#include <stdio.h>
int main() {
    int marks;
    printf("Enter your marks (0â€“100): ");
    scanf("%d", &marks);
    if (marks >= 95)
        printf("Grade: A+\n");
    else if (marks >= 85)
        printf("Grade: A\n");
    else if (marks >= 60)
        printf("Grade: B\n");
    else if (marks >=55 )
        printf("Grade: C\n");
    else if (marks >= 50)
        printf("Grade: D\n");
    else
        printf("Grade: F\n");

    return 0;
}
